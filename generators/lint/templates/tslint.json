{
  "rules": {
    "class-name": false,
    "curly": false,
    "eofline": false,
    "forin": false,
    "indent": [false, 4],
    "label-position": false,
    "label-undefined": false,
    "max-line-length": [false, 140],
    "no-arg": false,
    "no-bitwise": false,
    "no-console": [false,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": false,
    "no-debugger": false,
    "no-duplicate-key": false,
    "no-duplicate-variable": false,
    "no-empty": false,
    "no-eval": false,
    "no-string-literal": false,
    "no-trailing-whitespace": false,
    "no-unused-variable": false,
    "no-unreachable": false,
    "no-use-before-declare": false,
    "one-line": [false,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [false, "double"],
    "radix": false,
    "semicolon": false,
    "triple-equals": [false, "allow-null-check"],
    "variable-name": false,
    "whitespace": [false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
